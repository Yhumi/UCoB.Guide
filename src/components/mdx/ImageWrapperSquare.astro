---
import ImageWrapper from './ImageWrapper.astro';

const props = Astro.props;

interface Props {
  img: ImageMetadata;
  alt?: string;
  classdiv?: string;
  classimg?: string;
}
---

<ImageWrapper img={props.img} alt={props.alt} classdiv={props.classdiv} classimg={["w-96", "max-w-100", "aspect-square", props.classimg]}/>